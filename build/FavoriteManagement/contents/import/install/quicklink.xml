<sailpoint>

  <ImportAction name="merge">
    <Configuration name="SystemConfiguration">
      <Attributes>
        <Map>
          <entry key='quickLinkCategories'>
            <value>
              <List>
                <QuickLinkCategory name="FavoriteManagement" messageKey="FavoriteManagement" enabled="true" icon="quickLinkCategoryCustom" />
              </List>
            </value>
          </entry>
        </Map>
      </Attributes>
    </Configuration>
  </ImportAction>

  <QuickLink action="external" category="FavoriteManagement" messageKey="Manage my favorites" name="FavoriteManagement" ordering="4">
    <Attributes>
      <Map>
        <entry key="url" value="plugins/pluginPage.jsf?pn=FavoriteManagement"/>
      </Map>
    </Attributes>
    <QuickLinkOptions allowSelf="true">
      <DynamicScopeRef>
        <Reference class="sailpoint.object.DynamicScope" name="Everyone"/>
      </DynamicScopeRef>
    </QuickLinkOptions>
  </QuickLink>
<QuickLink action="external" category="FavoriteManagement" messageKey="Favorite Management" name="FavoriteManagement" ordering="4">
