      <Map>
        <entry key="url" value="plugins/pluginPage.jsf?pn=FavoriteManagement"/>
      </Map>
    </Attributes>
    <QuickLinkOptions allowSelf="true">
      <DynamicScopeRef>
        <Reference class="sailpoint.object.DynamicScope" name="Everyone"/>
      </DynamicScopeRef>
    </QuickLinkOptions>
  </QuickLink>
</sailpoint>

